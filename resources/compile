OUT_DIR=generated-evm


rm -rf $OUT_DIR
/opt/solc-0.4.24 --pretty-json --overwrite -o $OUT_DIR --bin --bin-runtime --abi --combined-json abi,asm,ast,bin,bin-runtime,clone-bin,compact-format,devdoc,hashes,interface,metadata,opcodes,srcmap,srcmap-runtime,userdoc $1
mv $OUT_DIR/combined.json $OUT_DIR/$1.json